{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "",
    "Issuer": "https://app.example.com",
    "Audience": "drive-api",
    "ExpiresInSeconds": 300
  },
  "Rt": {
    "ExpiresInHours": 720
  },
  "AuthCookie": {
    "Name": "auth",
    "MaxAgeHours": "720",
    "Domain": "localhost",
    "Path": "/api/auth/"
  },
  "Origin": {
    "Host": "localhost",
    "Port": "5100",
    "Path": "/api/auth/",
    "FederationSuccessPath" : "/federatedLogin"
  },
  "Google": {
    "Enabled" :  false,
    "ClientId": "",
    "ClientSecret": "",
    "CallbackPath": "/api/auth/web-google-callback"
  }
}
