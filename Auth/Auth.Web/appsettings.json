{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "",
    "Issuer": "https://app.example.com",
    "Audience": "drive-api",
    "ExpiresInSeconds": 900
  },
  "Rt": {
    "ExpiresInHours": 720.0,
    "SessionMaxAgeHours": 0.0
  },
  "AuthCookie": {
    "Name": "auth",
    "MaxAgeHours": "720"
  },
  "Login": {
    "MaxFailedPasswordLoginAttempts": 6,
    "AccountLockDurationInMinutes": 1,
    "LockoutStartsAfterAttempts": 3
  },
  "Register": {
    "MaxRegsitrationEmailsPerDay": 30,
    "EmailConfirmationTokenExpiresInHours": 72,
    "ResendConfirmationEmailCooldownInSeconds": 15,
    "EmailConfirmUrl": "https://localhost/confirmEmail"
  },
  "RegistrationEmail": {
    "From": "no-reply@cloudhatch.com",
    "Subject" : "Confirm your email"
  },
  "Origin": {
    "Host": "localhost",
    "FederationSuccessPath": "/federatedLogin"
  },
  "GoogleOAuth": {
    "Enabled" :  false,
    "ClientId": "",
    "ClientSecret": ""
  },
  "MicrosoftOAuth": {
    "Enabled": false,
    "ClientId": "",
    "ClientSecret": ""
  }
}
